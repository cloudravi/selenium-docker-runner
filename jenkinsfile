	pipeline{
		agent any
		stages{
			stage("Start Grid"){
				steps{
					sh "docker-compose up -d hub chrome firefox"
				}
			}
			stage("Running Test"){
				steps{
					sh "docker-compose up BookFlight"
				}

			}
			stage("Bring Down"){
				steps{
						sh "docker-compose down"
				}
			}
			
		}
	}